    "@babel/plugin-transform-runtime": "^7.10.4",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@testing-library/jest-dom": "^4.0.0",
    "@testing-library/react": "^9.1.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^24.8.0",
    "babel-polyfill": "^6.26.0",
